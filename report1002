
function add(1, 2) {
  var z = 1 + 2;
  return z;
}

function add(1, 2) {
  var z = 1 * 2;
  return z;
}


function lec01_02_ {
  var a = 2x;
  var sum;
  sum = add(10, a);
  alert('sum = ' + sum);
}

